{
  "name": "ytake/hack-terminal-nes-emulator",
  "description": "A HHVM/Hack terminal NES emulator",
  "keywords": ["hack", "terminal", "console", "nes", "emulator"],
  "homepage": "http://github.com/ytake/hack-terminal-nes-emulator",
  "license": "MIT",
  "require": {
    "hhvm": ">=4.44",
    "hhvm/hsl": "^4.25",
    "hhvm/hsl-experimental": "^4.25",
    "hhvm/hhvm-autoload": "^3.0",
    "facebook/xhp-lib": "^3.1",
    "facebook/hh-clilib": "^2.2.4"
  },
  "require-dev": {
    "hhvm/hacktest": "^2.0",
    "facebook/fbexpect": "^2.7",
    "hhvm/hhast": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Hes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HesTest\\": "tests/"
    }
  },
  "prefer-stable": true
}
